package day0108;

public class For_09 {

	public static void main(String[] args) {
		// 가로로 1~10까지 출력
		
		
		for(int x = 1; x <= 10; x++ ) {
			System.out.print(x + "  ");
		}
		
		System.out.println();
		
		
		//10 ~1까지 출력
		
		 
		for(int x = 10; x >= 1; x--) {
			System.out.print(x + "  ");
		}
		
		
		System.out.println();
		
		//5배수
		
		for(int x = 5; x <= 50 ; x += 5 ) {
		 System.out.print(x + "  ");
		}
		
		
		
		
	}

}
