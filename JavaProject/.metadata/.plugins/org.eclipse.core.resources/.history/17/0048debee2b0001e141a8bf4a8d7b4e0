package day0112;
/*
 * 음식명
 * 가격
 */
class Food {
	
	private String foodName; //이름
	private int fPrice; //가격
	
	static int no;
	
	//명시적 생성자(2개)
	public Food(String fo, int fp) {
		this.foodName = fo;
		this.fPrice = fp;
	}
	
	//출력문
	
	public void foodData() {
		System.out.println("[메뉴_]" + no);
		System.out.println("음식명 : " + this.foodName);
		System.out.println("가격 : " + this.foodName);
		System.out.println("----------------------------------");
	}
	
	
	
	
	
	
}

