package day0108;

public class For_10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i = 1;
		
		for(; i <=10; i++) {
			System.out.print(i + "   ");
		}
		
		System.out.println("\nfor문 빠져 나온 i의 값은 " + i ); //for문에서 빠져나오면 그 다음 값은 11이 추출됨.
	
        
		int a =10;
		
		for(; a <= 50; a += 5) {
			System.out.print(a + "   ");
		}
	
		System.out.println("\n빠져 나온 a값 : " + a);
	}

}
