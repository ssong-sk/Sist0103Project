package day0111;

class Student1 {
	private String stuName;
	private String hp;
	private int score;

	// 생성자 3개짜리
	public Student1(String name, String hp, int score) {
		this.stuName = name;
		this.hp = hp;
		this.score = score;
	}

	// 출력메서드

	public void writeData() {
		System.out.println("이름: " + stuName);
		System.out.println("전화번호: " + hp);
		System.out.println("점수: " + score);

	}

}

