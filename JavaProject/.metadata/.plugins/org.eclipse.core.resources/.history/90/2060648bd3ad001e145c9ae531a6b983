package day0108;

public class For_09 {

	public static void main(String[] args) {
		// 가로로 1~10까지 출력
		
		
		for(int x = 1; x <= 10; x++ ) {
			System.out.print(x + "  ");
		}
		
		System.out.println();
		//10 ~1까지 출력
		
		 
		for(int y = 10; y >= 1; y--) {
			System.out.print(y + "  ");
		}
		
		
		
	}

}
