package day0110;

class MyDate {
	
	private int year;
	private int month;
	private int day;
	
	//디폴트 생성자가 숨겨져있음
	
	//setter
	
	public void setYear(int y) {
		this.year = y;
	}
	
	public void setMonth(int m) {
		this.month = m;
	}
	
	public void setDay(int d) {
		this.day = d;
	}
	
	
	//getter
	
	public int getYear () {
		return year;
	}
	
	public int getMonth () {
		return month;
	}
	
	public int getDay () {
		return day;
	}
	
}

public class MyDate_14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
