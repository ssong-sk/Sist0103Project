package day0104;

public class book29_01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 변수선언
		
		int x; //int는 정수형의 기본 자료형,
		x = 10; // 변수 x에 1 값을 저장 , 10진수 정수의 저장을 위한 메모리 공간을 할당
		        //그릭 그 메모리공간의 이름은 x라고 하겠다는 뜻
		int y = 20; // 변수 y에 2 값을 저장
		int result = x + y;
		System.out.println(result);
		
		
	}

}
