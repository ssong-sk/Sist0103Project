package day0105;

public class OperTest_05 {

	public static void main(String[] args) {
		//  비교연산자 (=관계연산자) >, <, >=, <=, !=, ==
		// 논리연산자 &&(AND 두개 다), || (OR 둘중 하나), !(NOT)
		
		
		int a = 5, b = 3, c = 5;
		
		//결과값 true, false
		
		System.out.println(a > b);
		System.out.println(a == b);
		System.out.println(!(a == b));
		System.out.println(a == b);
		System.out.println(a == b);
		System.out.println(a == b);
		System.out.println(a == b);
	}

}
