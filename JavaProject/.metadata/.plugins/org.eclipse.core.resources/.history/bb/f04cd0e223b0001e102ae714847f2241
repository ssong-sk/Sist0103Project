package day0111;


class Artist {
	
	//변수명
	private String Name;
	private String Group;
	private String Song;
	
	static int cnt = 0;
	//명시적 생성자
	public void setArtist(String name, String group, String song) {
		this.Name = name;
		this.Group = group;
		this.Song = song;
	}
	
	//출력메서드 한번에 만들기
	
	public void ArtistData() {
		cnt++;
		System.out.println("**아티스트 정보_ " + cnt + "***");
		
	}
	
}
