package day0111;

class Apple {
	public Apple() {
		System.out.println("Apple__디폴트 생성자");

	}

}

class Banana {

	public Banana(String str) {
		System.out.println("생성자 호출 : " + str);
	}

}

class Orange {

	// 생성자 오버로딩 (생성자, 메서드명이 같게 생성)
	public Orange() {

	}

	public Orange() {

	}

	public Orange() {

	}
}

public class ConstTest_02 {

	public static void main(String[] args) {

	}

}
