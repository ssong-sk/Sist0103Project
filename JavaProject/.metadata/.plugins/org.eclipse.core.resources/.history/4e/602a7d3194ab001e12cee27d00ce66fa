package day0105;

public class SwitchTest_19 {

	public static void main(String[] args) {
		// switch문 예제 _ #1\
		
		
		int a = 1;
		
		
		//break가 없을경우 끝까지 실행
		
		switch(a) {
		case 1 : 
			System.out.println("a is 1");
		case 2 :
			System.out.println("a is 2");
		case 3 :
			System.out.println("a is 3");
			default :
				System.out.println("a is 입력값 없음");
		}

		
		
		
	}

}
