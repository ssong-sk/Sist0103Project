package day0110;

class Car {

	//필드 선언
	String model;
	int speed;
	
	//생성자 선언
	Car(String model) {
		this.model = model;
	}
	
	//메소드 선언
	void setSpeed(int speed) {
		this.speed = speed;
	}
	
	
}
